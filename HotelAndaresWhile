public class HotelAndaresWhile {
    public static void main(String[] args) {
        int i = 20; // ComeÃ§amos do andar 20
        while (i >= 1) {
            if (i == 13) {
                i--; // Pula o andar 13
                continue;
            }
            System.out.println("Andar: " + i);
            i--;
        }
    }
}
